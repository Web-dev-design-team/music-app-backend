{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./",
          "@models": "./src/database/models",
          "@validations": "./src/validations",
          "@auth": "./src/auth",
          "@env": "./src/env"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
